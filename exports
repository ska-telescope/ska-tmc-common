export PPATHS=$HOME/python_310
export PATH=$PPATHS/poetry/bin:$PATH
export PATH=$PPATHS/bin:$PATH
export LD_LIBRARY_PATH=$PPATHS/lib:$LD_LIBRARY_PATH
export LIBRARY_PATH=$PPATHS/lib:$LIBRARY_PATH
export INCLUDE=$PPATHS/include:$INCLUDE
export C_INCLUDE_PATH=$PPATHS/include:$C_INCLUDE_PATH
export CPATH=$PPATHS/include:$PPATHS/include/tango:$CPATH
export CPLUS_INCLUDE_PATH=$PPATHS/include:$CPLUS_INCLUDE_PATH
export PKG_CONFIG_PATH=$PPATHS/lib/pkgconfig:$PKG_CONFIG_PATH
export PYTHONPATH=$HOME/python_310/lib/python3.10/site-packages:$PYTHONPATH
